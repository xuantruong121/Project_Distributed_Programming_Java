<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="rootContainer" fx:controller="iuh.fit.qlksfxapp.controller.ItemController.EmptyController" alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <ImageView fx:id="imageEmpty" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@../../images/icon/no_results.png" />
        </image>
    </ImageView>
    <Label fx:id="messageLabel" styleClass="empty-label" text="Không có kết quả">
      <font>
         <Font size="20.0" />
      </font></Label>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
